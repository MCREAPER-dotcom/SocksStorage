<Window x:Class="SocksStore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SocksStore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>

        <Button Content="PageSocks" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="25" Width="auto" Background="#d6ebb0" Command="{Binding LoadPageSocks, Mode=OneTime}"/>
        <Button Content="PageMySocks" HorizontalAlignment="Left" Margin="70,10,0,0" VerticalAlignment="Top" Height="25" Width="auto" Background="#d6ebb0" Command="{Binding LoadPageMySocks, Mode=OneTime}"/>

        <Frame x:Name="OutputView" NavigationUIVisibility="Hidden" Margin="0,40,0,1" />
    </Grid>
</Window>
